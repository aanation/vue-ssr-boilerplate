{
  "name": "vue-ssr",
  "description": "vue webpack and express ssr-server by 42px",
  "version": "1.0.0",
  "author": "42px <2822698@gmail.com>",
  "private": true,
  "scripts": {
    "dev-spa": "cross-env NODE_ENV=development webpack-dev-server --config webpack.spa.config.js --open --hot",
    "build-spa": "cross-env NODE_ENV=production webpack --config webpack.spa.config.js  --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test karma start tests/unit/karma.conf.js --single-run",
    "tests": "npm run unit",
    "build-client": "cross-env NODE_ENV=production webpack --config webpack.ssr.client.config.js  --progress --hide-modules",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack.ssr.server.config.js  --progress --hide-modules",
    "build": "npm run build-client && npm run build-server",
    "dev": "cross-env NODE_ENV=development node render-server/index.js",
    "render-server": "cross-env NODE_ENV=production node render-server/index.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "jquery": "^3.2.1",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
