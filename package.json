{
  "name": "vue-ssr",
  "description": "vue webpack and express ssr-server",
  "version": "1.0.0",
  "author": "42px <2822698@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.spa.config.js --open --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.spa.config.js  --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test karma start tests/unit/karma.conf.js --single-run",
    "tests": "npm run unit",
    "build-client": "cross-env NODE_ENV=production webpack --config webpack.ssr.client.config.js  --progress --hide-modules",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack.ssr.server.config.js  --progress --hide-modules"
  },
  "dependencies": {
    "chai": "^4.0.2",
